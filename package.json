{
  "name": "ai-time-management",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "start": "npm run start:dev",
    "start:dev": "chmod +x start.sh && ./start.sh dev",
    "start:docker": "chmod +x start.sh && ./start.sh docker",
    "start:build": "chmod +x start.sh && ./start.sh build",
    "stop": "chmod +x stop.sh && ./stop.sh",
    "start:windows": "start.bat dev",
    "stop:windows": "start.bat stop"
  },
  "dependencies": {
    "antd": "^5.12.0",
    "axios": "^1.6.0",
    "date-fns": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^1.0.0",
    "autoprefixer": "^10.4.16",
    "happy-dom": "^12.10.3",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^1.0.0"
  }
}
